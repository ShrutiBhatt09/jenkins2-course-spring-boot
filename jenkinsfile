pipeline {
  agent any
  stages {
    stage('Source') { 
      steps {
			// Checkout code 
			checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/ShrutiBhatt09/jenkins2-course-spring-boot.git']]])
      
      }
    }
    def project_path = "spring-boot-samples\spring-boot-sample-atmosphere"
    dir(project_path)
    {
         stage('Compile') { 
      tools {
        // Specify Tool Name from your global tool configuration
		jdk 'JDK'
        maven 'Maven3'
      }
      steps {
			// Some Step
      }
    }
    
    
    }
  
  }
}
